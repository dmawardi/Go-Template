
{{define "content"}}
    {{if .FormPage}}
     <div class="content-container">
        <div class="row">
            <div>
                <h1>Form</h1>
                {{template "form" .FormData}}
            </div>
        </div>
    </div>
    {{else if .UserEditFormPage}}
    <div class="content-container">
        <div>
            <div>
                <h1>Form</h1>
                {{template "form" .FormData}}
            </div>
        </div>
    </div>
{{else}}
    <div class="content-container">
        <div>
            <div>
                <h1>Form</h1>
                <p>Form page not detected</p>
            </div>
        </div>
{{end}}
   
{{end}}
